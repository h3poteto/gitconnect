<%= simple_form_for(@clients, url: clients_setting_path) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "更新" %>
  </div>
<% end %>

<%= link_to account_omniauth_authorize_path(:github) do %>
  <span class="octicon octicon-mark-github btn btn-success"> Sign in with github</span>
<% end %>
